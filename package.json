    "seed:user": "ts-node ./src/sequelize-dir/seeders/users.seeder.ts",
    "seed:state": "ts-node ./src/sequelize-dir/seeders/states.seeder.ts",
    "seed:city": "ts-node ./src/sequelize-dir/seeders/cities.seeder.ts",
    "seed": "npm run seed:user && npm run seed:state && npm run seed:city"
  },
